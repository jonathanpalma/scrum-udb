{
  "name": "express-ts-poc",
  "version": "0.0.1",
  "description": "Backend for Frontend",
  "main": "index.js",
  "scripts": {
    "dev:build": "tsc -w",
    "dev:run": "nodemon build/index.js",
    "dev": "concurrently npm:dev:*",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "prettier": "prettier \"**/*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)\"",
    "start:build": "tsc",
    "start:run": "node build/index.js",
    "start": "npm run start:build && npm run start:run",
    "validate": "npm run lint && npm run prettier -- --list-different"
  },
  "author": "Jonathan Palma <tanpalma04@gmail.com> (http://jonathanpalma.me)",
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/express-winston": "^3.0.3",
    "@types/node": "^12.6.8",
    "@types/sequelize": "^4.28.4",
    "body-parser": "^1.20.1",
    "concurrently": "^4.1.1",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.18.2",
    "express-winston": "^3.2.1",
    "lodash": "^4.17.14",
    "nodemon": "^1.19.1",
    "pg": "^7.12.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.12.2",
    "typescript": "^3.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-prettier": "^3.1.0",
    "prettier": "^1.18.2"
  }
}
